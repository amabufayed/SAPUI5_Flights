{
	"_version": "1.7.0",
	"sap.app": {
		"id": "SAPUI_Flights",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"Z_MBES_FLIGHTS02_SRV": {
				"uri": "/sap/opu/odata/sap/Z_MBES_FLIGHTS02_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "SAPUI_Flights.view.App",
			"id": "app",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {},
				"sap.f": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "SAPUI_Flights.i18n.i18n"
				}
			},
			"": {
				"dataSource": "Z_MBES_FLIGHTS02_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "SAPUI_Flights.view",
				"controlId": "layout",
				"controlAggregation": "midColumnPages",
				"bypassed": {
					"target": [
						"home",
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": [
						"home",
						"welcome"
					]
				},
				{
					"pattern": "flight-detail/{connid}",
					"name": "flightDetail",
					"target": [
						"home",
						"flightDetail"
					]
				}
			],
			"targets": {
				"welcome": {
					"viewName": "Welcome",
					"controlAggregation": "midColumnPages"
				},
				"home": {
					"viewName": "Home",
					"controlAggregation": "beginColumnPages"
				},
				"flightDetail": {
					"viewName": "FlightDetail",
					"controlAggregation": "midColumnPages"
				}
			}
		}
	}
}